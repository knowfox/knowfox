{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/knowfox/entangle"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-debugbar": "^2.3",
        "bugsnag/bugsnag-laravel": "2.0",
        "cebe/markdown": "^1.1",
        "doctrine/dbal": "^2.5",
        "evernote/evernote": "^1.26",
        "folklore/graphql": "^1.0",
        "genealabs/laravel-caffeine": "^0.5.0",
        "guzzlehttp/guzzle": "^6.2",
        "kalnoy/nestedset": "^4.3",
        "knowfox/drupal7": "dev-master",
        "knowfox/entangle": "^1.2.2",
        "laravel/framework": "^5.5",
        "laravel/passport": "^4.0",
        "laravel/tinker": "~1.0",
        "mpociot/versionable": "^2.1",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.5",
        "rtconner/laravel-tagging": "^2.2",
        "symfony/yaml": "^3.3",
        "vipnytt/opmlparser": "dev-master"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^6.4",
        "symfony/dom-crawler": "^3.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Knowfox\\": "app/",
            "Knowfox\\Entangle\\": "packages/knowfox/entangle/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
